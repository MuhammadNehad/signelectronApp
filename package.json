{
  "type": "commonjs",
  "name": "sign",
  "version": "0.2.0",
  "description": "for emps signing",
  "main": "main.js",
  "scripts": {
    "electron": "electron .",
    "dist": "electron-builder",
    "publish": "electron-builder --win -p always",
    "publishn": "electron-builder --win -p never",
    "postinstall": "electron-builder install-app-deps",
    "rollups":"rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MuhammadNehad/signelectronApp.git"
  },
  "author": "Muhammad Nehad",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MuhammadNehad/signelectronApp.git/issues"
  },
  "homepage": "https://github.com/MuhammadNehad/signelectronApp.git#readme",
  "dependencies": {
    "@electron-forge/publisher-github": "^6.0.0-beta.63",
    "asar": "^3.1.0",
    "bootstrap": "^5.1.0",
    "datatables.net": "^1.10.25",
    "datatables.net-dt": "^1.1.0",
    "jquery": "^3.6.0",
    "update-electron-app": "^2.0.1"
  },
  "devDependencies": {
    "electron": "^13.2.3",
    "electron-builder": "^22.11.7",
    "electron-rebuild": "^3.2.7"
  },
  "build": {
    "appId": "com.sgnempapp.app",
    "productName": "sign",
    "target": "NSIS",
    "directories": {
      "output": "build"
    },
    "win": {
      "target":["nsis"],
      "publish": [
        {
          "provider": "github",
          "owner": "MuhammadNehad",
          "repo": "signelectronApp"
        }
      ]
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "perMachine": true
    }
  }
}
